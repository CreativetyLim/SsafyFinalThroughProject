<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssafy.happyhouse.dao.CctvDAO">

	<insert id="regist">
		insert into cctv(roadName, lat, lon) values(#{roadName},#{lat},#{lon})
	</insert>
	
	<select id="list" resultType="com.ssafy.happyhouse.dto.cctv">
		select * from cctv
	</select>
</mapper>
