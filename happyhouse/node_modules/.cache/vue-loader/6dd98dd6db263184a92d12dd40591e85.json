\" v-model=\"User.uId\">\n                        </v-text-field>\n                <v-text-field cla\n                ss=\"pl-3 pr-3\" label=\"Password\" prepend-icon=\"mdi-lock\"\n                            required type=\"password\" v-model=\"User.uPw\">\n                        </v-text-field>\n                <v-btn @click=\"userLogin\" class=\"mt-3\" color=\"indigo\" outlined>\n                            Login\n                        </v-btn>\n            </v-form>\n        </div>\n        <b-alert variant=\"danger\" :show=\"showAlert\">{{ errMsg }}</b-alert>\n    </v-row>\n</template>\n\n<script>\n//import {router} from \"../../router/index\";\nimport axios from 'axios';\nexport default {\n    name: 'Login',\n    data() {  \n        return {\n            User: {\n                uId: '',\n                uPw: '',\n            },\n            showAlert: false,\n            errMsg: ''\n        } \n    },\n    methods: {\n        userLogin(){\n            if ( this.uId == '' ) {\n                this.showAlert = true;\n                this.errMsg = 'Please enter your username';\n                return;\n            }\n            if ( this.uPw == '' ) {\n                this.showAlert = true;\n                this.errMsg = 'Please enter the password';\n                return;\n            }\n            this.showAlert = false;\n\n            axios.Post(`http://localhost:9999/happyhouse/api/user/login`, {\n                uId: this.User.uId,\n                uPw: this.User.uPw\n            })\n            .then(res => {\n                alert(\"로그인 되었습니다.\");\n                console.dir(\"success\")\n                console.log(res)\n            }).catch(e => {\n                console.error(e)\n            })\n        }\n    },\n}\n</script>"]}]}